<!-- External script with nonce -->
<script nonce="<%= data.nonce %>" src="/assets/js/script.js"></script>

<!-- Inline script with nonce -->
<script nonce="<%= data.nonce %>">
    localStorage.setItem("encid", "<%= data.response.meta.encid %>");
    setTimeout(()=>{
        window.location.href = "<%= data.response.meta.derivLoginURL %>";
    },1000)
</script>

